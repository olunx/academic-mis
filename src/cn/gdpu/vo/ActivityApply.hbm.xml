<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.gdpu.vo">
	<class name="ActivityApply" table="t_activityApply" >
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<many-to-one name="activity" column="activityId" />
		<many-to-one name="group" column="groupId" />
		<property name="applytime" />
		<property name="endtime" />
		<many-to-one name="operator" column="assistantId" />
		<property name="record" />
		<set name="applicants" lazy="false" order-by="id asc">
			<key column="aaId" />
			<one-to-many class="Student" />
		</set>
		<property name="status" />
		<many-to-one name="opus" column="opusId" unique="true"/>
		
	</class>
</hibernate-mapping>
